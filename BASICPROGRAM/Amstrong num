#include<stdio.h>
#include<math.h>
int main(){
    int d,num=0,r=0,c=0;
    double sum=0;
    printf("Enter a number: ");
    scanf("%d",&d);
    num=d;
    while(num>0){
        num=num/10;
        c++;
        
    }
    sum=0;
    num=d;
    while(num>0){
        r=num%10;
        sum+=pow(r,c);
        num=num/10;
        
    }
    if ((int)sum==d){
        printf("Amstrong number");
    }
    else{
        printf("Not a amstrong number");
    }
    return 0;
}
##ERROR IS TEMP VARIABLE
MUST USE=D=NUM=0,REMAINDER=0,COUNT=0
NUM=D
