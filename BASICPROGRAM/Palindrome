#include <stdio.h>
#include <string.h>
int main(){
    char w[50];
    printf("Enter a string: ");
    scanf("%s",w);
    int d=strlen(w);
    for(int i=0;i<=d;i++){
        if(w[i]==w[d-1]){
            printf("its a palindrome");
        }
        else
        {
            printf("not a palindrome");
            
        }
        return 0;
    }
}
